<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="300" height="400" viewBox="0 0 300 400">
  <style>
    .receipt {
      fill: #ffffff;
      stroke: #cccccc;
      stroke-width: 2;
    }
    .receipt-text {
      font-family: 'Arial', sans-serif;
      font-size: 16px;
      fill: #333333;
    }
    .receipt-title {
      font-family: 'Arial', sans-serif;
      font-size: 20px;
      font-weight: bold;
      fill: #333333;
    }
    .receipt-line {
      stroke: #dddddd;
      stroke-width: 1;
    }
    .receipt-logo {
      fill: #5c6bc0;
    }
  </style>

  <!-- Receipt Background -->
  <rect class="receipt" x="20" y="20" width="260" height="360" rx="5" ry="5" />
  
  <!-- Receipt Header -->
  <text class="receipt-title" x="150" y="50" text-anchor="middle">STORE RECEIPT</text>
  <line class="receipt-line" x1="30" y1="65" x2="270" y2="65" />
  
  <!-- Receipt Logo -->
  <circle class="receipt-logo" cx="50" cy="40" r="15" />
  
  <!-- Receipt Content -->
  <text class="receipt-text" x="40" y="90">Date: 05/15/2025</text>
  <text class="receipt-text" x="40" y="115">Items:</text>
  
  <line class="receipt-line" x1="40" y1="125" x2="260" y2="125" />
  <text class="receipt-text" x="45" y="145">Item 1</text>
  <text class="receipt-text" x="240" y="145" text-anchor="end">$12.99</text>
  
  <line class="receipt-line" x1="40" y1="155" x2="260" y2="155" />
  <text class="receipt-text" x="45" y="175">Item 2</text>
  <text class="receipt-text" x="240" y="175" text-anchor="end">$8.50</text>
  
  <line class="receipt-line" x1="40" y1="185" x2="260" y2="185" />
  <text class="receipt-text" x="45" y="205">Item 3</text>
  <text class="receipt-text" x="240" y="205" text-anchor="end">$24.75</text>
  
  <line class="receipt-line" x1="40" y1="225" x2="260" y2="225" stroke-width="2" />
  
  <text class="receipt-text" x="45" y="245">Subtotal</text>
  <text class="receipt-text" x="240" y="245" text-anchor="end">$46.24</text>
  
  <text class="receipt-text" x="45" y="265">Tax</text>
  <text class="receipt-text" x="240" y="265" text-anchor="end">$3.70</text>
  
  <line class="receipt-line" x1="170" y1="275" x2="260" y2="275" />
  
  <text class="receipt-title" x="45" y="295">Total</text>
  <text class="receipt-title" x="240" y="295" text-anchor="end">$49.94</text>
  
  <line class="receipt-line" x1="30" y1="315" x2="270" y2="315" />
  
  <!-- Receipt Footer -->
  <text class="receipt-text" x="150" y="340" text-anchor="middle" font-size="12">Thank you for your purchase!</text>
  <text class="receipt-text" x="150" y="360" text-anchor="middle" font-size="12">www.blockreceipt.ai</text>
</svg>